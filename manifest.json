{
    "manifest_version": 3,
    "browser_specific_settings": {
        "gecko": {
            "id": "nBolsonaro@oversizebrazil.me",
            "data_collection_permissions": {
                "required": [
                    "none"
                ]
            }
        }
    },
    "name": "nBolsonaro",
    "description": "A Chrome extension which replaces all images on all pages to Jair Messias Bolsonaro.",
    "version": "1.1",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "nbolsonaro.js"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }
}